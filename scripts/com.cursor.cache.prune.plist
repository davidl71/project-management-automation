<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.cursor.cache.prune</string>
    
    <key>Comment</key>
    <string>Cursor Cache Pruner - Runs daily at 3am to clean old cache data</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/Volumes/SSD1_APFS/project-management-automation/scripts/cursor-cache-prune.sh</string>
        <string>--execute</string>
    </array>
    
    <!-- Run daily at 3:00 AM -->
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>3</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>
    
    <key>StandardOutPath</key>
    <string>/tmp/cursor-cache-prune.out.log</string>
    
    <key>StandardErrorPath</key>
    <string>/tmp/cursor-cache-prune.err.log</string>
    
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>
</dict>
</plist>

